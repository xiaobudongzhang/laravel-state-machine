{
    "name": "xiaobudongzhang/laravel-state-machine",
    "description": "Winzou State Machine service provider for Laravel",
    "keywords": [
        "laravel",
        "state",
        "event",
        "statemachine"
    ],
    "homepage": "https://github.com/xiaobudongzhang/laravel-state-machine",
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^7.0",
        "winzou/state-machine": "^0.4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.4 | ^9.0",
        "symfony/process": "^4.3 | ^5.0",
        "xiaobudongzhang/state-machine": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "Sebdesign\\SM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sebdesign\\SM\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Sebdesign\\SM\\ServiceProvider"
            ],
            "aliases": {
                "StateMachine": "Sebdesign\\SM\\Facade"
            }
        }
    }
}
